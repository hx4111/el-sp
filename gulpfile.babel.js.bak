import gulp from 'gulp'
import run from 'gulp-run'
import webpack from 'webpack'
import webpackConfig from './webpack.config.js'

gulp.task('default', () => {
    run('electron .').exec()
    gulp.start('webpack')
})

gulp.task('webpack', () => {
    webpack(webpackConfig, (err, state) => {
        console.info(state.toString())
    })
})
